client
dev tun
proto tcp-client

# Replace with your server's actual hostname or IP
# remote vpn.easytem.com 443
# VM1
remote 169.57.159.36 443
# VM2
#remote 169.57.179.136 443

resolv-retry infinite
nobind
persist-key
persist-tun
ping 10
ping-restart 60

tun-mtu 1500
mssfix 1360

block-outside-dns
register-dns

# Certificate and key settings
remote-cert-tls server
cipher AES-256-CBC
auth SHA512
verb 3
key-direction 1
#auth-user-pass

sndbuf 393216
rcvbuf 393216

compress

